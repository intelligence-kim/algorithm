   "source": [
    "# 그리디 알고리즘\n",
    "1. 리스트를 정렬 후 각 단계 전까지의 합을 계속해서 더해준다."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 25,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "32\n"
     ]
    }
   ],
   "source": [
    "answer = 0\n",
    "N = int(input())\n",
    "P = list(map(int,input().split()))\n",
    "P.sort()\n",
    "\n",
    "for i in range(N):\n",
    "    answer+=sum(P[:i+1])\n",
    "print(answer)\n"
   ]
  }
