    "def fib_optimized(n):\n",
    "    # 여기에 코드를 작성하세요\n",
    "    current = 1\n",
    "    previous = 1\n",
    "\n",
    "    for i in range(n-2):\n",
    "        current, previous = current+previous, current\n",
    "    return current\n",
    "\n",
    "\n",
    "# 테스트 코드\n",
    "print(fib_optimized(16))\n",
    "print(fib_optimized(53))\n",
    "print(fib_optimized(213))\n"
